{
  "compilerOptions": {
    "experimentalDecorators": true, // <------开启装饰器语法
    "strictNullChecks": false, // <--------为true是定义class必须给属性赋值
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowUmdGlobalAccess": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@by-l/react-form": [
        "src"
      ],
      "@by-l/react-form/*": [
        "src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*"
  ]
}